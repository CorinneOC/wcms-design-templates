<!doctype html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=1.0, minimum-scale=1.0, maximum-scale=2.0">
    <title>{{ title }} - University of California, Santa Cruz</title>
    <meta content="{{ description }}" name="description">
    <meta content="{{ keywords }}" name="keywords">
  
    {{> global-head}}

</head>


<body class="{{#if classes}}{{classes}}{{/if}}">

  

            <!-- Do not show banner -->
    
<div id="wrap">
  <div id="container">
    <div id="content">
    
     
      <div role="banner" class="site-header">

        {{> global-header}}

        {{!-- For the titles sample page, ignore everything else. --}}
        {{#titles}}
          {{> header-titles }}
        {{/titles}}
        {{!-- Not the titles page, show the full goods. --}}
        {{^titles}}
          {{!-- Campus home header --}}
          {{#www}}
            {{> header-www }}    
          {{/www}}
          {{!-- Department/Division header --}}
          {{^www}}
            {{> header-dept }}
          {{/www}}
          {{!-- Breadcrumbs if we're not on a front page. --}}
          {{^front}}
              <div class="row breadcrumbs">
                  <p><a href="../index.html">Home</a> / <a href="http://example.com/">Directory</a> / This page</p>
              </div>
          {{/front}}
        {{/titles}}

      </div>

      {{#front}}
          <div class="row">
            {{> carousel }}
          </div>
      {{/front}}
    
    <div id="main" role="main">

      {{ body }}

    </div>

    
    <div class="row" id="footer" role="footer">
          <p class="links">
            <a href="about/contact.html">Feedback</a>
          </p>
          <p class="links">
            <a href="about/contact.html">Contact</a>
          </p>
          <p class="links">
            <a href="about/contact.html">Information</a>
          </p>

          <p class="address home">UC Santa Cruz, 1156 High Street, Santa Cruz, Ca 95064<br>
Â©2014 Regents of the University of California. All Rights Reserved.</p>
    </div>

    <div class="row legal">
      <p><a href="http://its.ucsc.edu/terms/">Privacy Policy and Terms of Use</a></p>
      <p><b>Page last built:</b> ##DATE##</p>
    </div>
    
    

    </div><!-- /content-->        
  </div><!-- /container -->
</div><!-- /wrap -->


{{> footer-scripts-global}}

{{#front}}  
  {{> footer-scripts-carousel}}
{{/front}}

</body>
</html>