{
    "name": "responsive-css-only",
    "version": "1.1.0",
    "authors": [
        "Rob Knight <raknight@ucsc.edu>"
    ],
    "description": "UCSC responsive templates 2014",
    "main": "./src/index.html",
    "keywords": [
        "responsive"
    ],
    "license": "MIT",
    "homepage": "index.html",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "vendor",
        "test",
        "tests"
    ],
    "dependencies": {
        "jquery": "~2.1.0",
        "modernizr": "~2.7.1",
        "owlcarousel": "~1.3.2",
        "fancybox": "~2.1.5"
    },
    "devDependencies": {},
    "overrides": {
        "fancybox": {
            "main": "source/**"
        },
        "modernizr": {
            "main": "modernizr.js"
        },
        "owlcarousel": {
            "main": "owl-carousel/**"
        }
    }
}